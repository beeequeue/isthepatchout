{
  "name": "isthepatchout",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "homepage": "https://isthepatchout.com",
  "repository": "https://github.com/BeeeQueue/isthepatchout",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "engines": {
    "node": "^20"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,vue,html,json}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "lint": "eslint .",
    "postinstall": "simple-git-hooks",
    "psql": "PGPASSWORD=postgres psql -h localhost -p 54322 -U postgres -f",
    "serve": "nuxi preview",
    "supabase:start": "supabase start -x storage-api,imgproxy,inbucket,studio,deno-relay,edge-runtime,logflare",
    "typecheck": "nuxi typecheck"
  },
  "resolutions": {
    "@types/node": "20.11.20"
  },
  "dependencies": {
    "@vueuse/core": "10.8.0",
    "canvas-confetti": "1.9.2",
    "date-fns": "3.3.1",
    "dotaver": "1.0.1",
    "fathom-client": "3.6.0",
    "fetch-unfiller": "0.2.0",
    "p-queue": "8.0.1",
    "pino": "8.19.0",
    "remeda": "1.44.1"
  },
  "devDependencies": {
    "@beequeue/eslint-plugin": "4.0.0",
    "@morev/vue-transitions": "2.3.6",
    "@nuxtjs/google-fonts": "3.1.3",
    "@nuxtjs/supabase": "1.1.6",
    "@supabase/realtime-js": "2.9.3",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/canvas-confetti": "1.6.4",
    "@types/node": "20.11.20",
    "@vercel/speed-insights": "1.0.10",
    "@vite-pwa/nuxt": "0.5.0",
    "@vueuse/nuxt": "10.8.0",
    "@windicss/plugin-animations": "1.0.9",
    "browserslist": "4.23.0",
    "dotenv": "16.4.5",
    "esbuild-plugin-browserslist": "0.11.1",
    "eslint": "8.56.0",
    "eslint-define-config": "2.1.0",
    "lint-staged": "15.2.2",
    "myzod": "1.11.0",
    "nuxt": "3.10.3",
    "nuxt-icon": "0.6.8",
    "nuxt-security": "1.2.0",
    "nuxt-windicss": "2.6.1",
    "ofetch": "1.3.3",
    "pino-http": "9.0.0",
    "pino-pretty": "10.3.1",
    "prettier": "3.2.5",
    "simple-git-hooks": "2.9.0",
    "typescript": "5.3.3",
    "vue": "3.4.19",
    "vue-tsc": "1.8.27"
  },
  "pnpm": {
    "overrides": {
      "array-includes": "npm:@nolyfill/array-includes@latest",
      "array.prototype.flat": "npm:@nolyfill/array.prototype.flat@latest",
      "array.prototype.flatmap": "npm:@nolyfill/array.prototype.flatmap@latest",
      "array.prototype.tosorted": "npm:@nolyfill/array.prototype.tosorted@latest",
      "es-iterator-helpers": "npm:@nolyfill/es-iterator-helpers@latest",
      "has": "npm:@nolyfill/has@latest",
      "hasown": "npm:@nolyfill/hasown@latest",
      "isarray": "npm:@nolyfill/isarray@latest",
      "object.assign": "npm:@nolyfill/object.assign@latest",
      "object.entries": "npm:@nolyfill/object.entries@latest",
      "object.fromentries": "npm:@nolyfill/object.fromentries@latest",
      "object.hasown": "npm:@nolyfill/object.hasown@latest",
      "object.values": "npm:@nolyfill/object.values@latest",
      "string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@latest"
    }
  }
}
