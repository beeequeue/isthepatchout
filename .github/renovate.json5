{
  extends: ["config:js-app", "helpers:disableTypesNodeMajor"],
  prHourlyLimit: 5,
  prConcurrentLimit: 5,
  branchConcurrentLimit: 5,
  schedule: ["before 11am"],
  labels: ["dependencies"],
  baseBranches: ["main"],
  packageRules: [
    {
      matchUpdateTypes: ["patch", "minor"],
      matchManagers: ["npm"],
      automerge: true,
    },
    {
      sourceUrlPrefixes: ["https://github.com/vitejs/vite"],
      groupName: "Vite",
    },
    {
      packagePatterns: ["lint"],
      groupName: "Linting",
    },
  ],
}
